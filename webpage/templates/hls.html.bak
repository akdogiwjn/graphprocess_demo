		function draw_bfs(){
		
			var myChart = echarts.init(document.getElementById('hls_bardata'));
			var colors = ['#000000', '#FF0000', '#4A4AFF', '#ADADAD'];
			option = {
			color: colors,
			tooltip: {
				trigger: 'axis',
				axisPointer: {
					type: 'cross'
				},
				formatter:function(datas){
					var res='';// = datas[0].name + '<br/>', val;
					for(var i = 0, length = datas.length; i < length; i++) {
						var rate = datas[i].value/datas[0].value-1;
						rate = rate.toFixed(2);
						var name=''+datas[i].seriesName;
						if(name=='通用HLS'){
							val = datas[i].value+'MTEPS';
						}
						else if(name=='面向图计算的HLS'){
							val = datas[i].value+'MTEPS&nbsp&nbsp&nbsp&nbsp<font color=\'#FFD306\'>提升'+rate+'倍</font>';
						}
						else if(name=='中期指标'){
							val = datas[i].value+'MTEPS&nbsp&nbsp&nbsp&nbsp<font color=\'#FFD306\'>提升30%</font>';
						}
						else if(name=='最终指标'){
							val = datas[i].value+'MTEPS&nbsp&nbsp&nbsp&nbsp<font color=\'#FFD306\'>提升70%</font>';
						}
						res += datas[i].marker+datas[i].seriesName + '：' + val + '<br/>';
					}
					return res;
				},
			},
			grid: {
				left: '10%',
				right: '10%'
			},
			toolbox: {
				feature: {
					dataView: {show: true, readOnly: true},
					restore: {show: false},
					saveAsImage: {show: true}
				}
			},
			legend: {
				data:['通用HLS','中期指标','面向图计算的HLS','最终指标']
			},
			xAxis: [
				{
					type: 'category',
					axisTick: {
						aignWithLabel: true
					},
					data: myColumns.x_data
				}
			],
			yAxis: [
				{
					type: 'value',
					name: 'MTEPS',
					min: 0.0,
					max: 500,
					position: 'left',
					axisLine: {
						lineStyle: {
							color: colors[0]
						}
					},
					axisLabel: {
						formatter: '{value} '
					}
				},
				{
					type: 'value',
					name: '',
					min: 0.0,
					max: 500,
					position: 'right',
					axisLine: {
						lineStyle: {
							color: colors[1]
						}
					},
					axisLabel: {
						formatter: '{value} '
					}
				}
			],
			series: [
				{
					name:'通用HLS',
					type:'bar',
					data: myColumns.spatial_data
				},
				{
					name:'中期指标',
					type:'bar',
					data: myColumns.midterm_data
				},
				{
					name:'面向图计算的HLS',
					type:'bar',
					yAxisIndex: 1,
					data: myColumns.hls_data,
				},
				{
					name:'最终指标',
					type:'bar',
					data: myColumns.final_data
				}
			]
		};
		
		myChart.setOption(option);
	}

	</script>

