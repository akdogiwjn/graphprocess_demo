			function select_banning(){
				if(!($("#hls_step").val()==='Compute'||$("#hls_step").val()==='Reduce'||$("#hls_step").val()==='Apply')){
					$('#hls_hw').find("option:selected").attr("selected", false);

					$("#hls_hw option[value='small_small']").attr("disabled",true);
					$("#hls_hw option[value='small']").attr("disabled",true);
					$("#hls_hw option[value='big']").attr("disabled",true);
				}
				else if($("#hls_algorithm").val()==='BFS'&&$("#hls_step").val()==='Apply'){
					if($("#hls_hw").val()==='big'){$('#hls_hw').find("option:selected").attr("selected", false);}

					$("#hls_hw option[value='small_small']").attr("disabled",false);
					$("#hls_hw option[value='small']").attr("disabled",false);
					$("#hls_hw option[value='big']").attr("disabled",true);
				}
				else if($("#hls_algorithm").val()==='PageRank'&&$("#hls_step").val()==='Reduce'){
					if($("#hls_hw").val()==='small_small'){$('#hls_hw').find("option:selected").attr("selected", false);}

					$("#hls_hw option[value='small_small']").attr("disabled",true);
					$("#hls_hw option[value='small']").attr("disabled",false);
					$("#hls_hw option[value='big']").attr("disabled",false);
				}
				else if($("#hls_algorithm").val()==='SSSP'&&$("#hls_step").val()==='Apply'){
					if($("#hls_hw").val()==='big'){$('#hls_hw').find("option:selected").attr("selected", false);}

					$("#hls_hw option[value='small_small']").attr("disabled",false);
					$("#hls_hw option[value='small']").attr("disabled",false);
					$("#hls_hw option[value='big']").attr("disabled",true);
				}
				else{
					$("#hls_hw option[value='small_small']").attr("disabled",false);
					$("#hls_hw option[value='small']").attr("disabled",false);
					$("#hls_hw option[value='big']").attr("disabled",false);
				}
				
				if(!($("#hls_step").val()==='Compute'||$("#hls_step").val()==='Reduce'||$("#hls_step").val()==='Apply')){
					$('#hls_code').find("option:selected").attr("selected", false)

					$("#hls_code option[value='中间代码']").attr("disabled",true);
					$("#hls_code option[value='Verilog代码']").attr("disabled",true);
				}
				else{
					$("#hls_code option[value='中间代码']").attr("disabled",false);
					$("#hls_code option[value='Verilog代码']").attr("disabled",false);
				}

			}
			$("#hls_algorithm").change(function (data) {
				select_banning();
			});
			$("#hls_step").change(function (data) {
				select_banning();
			});
			$("#hls_code").change(function (data) {
				select_banning();
			});
			$("#hls_hw").change(function (data) {
				select_banning();
			});
	

			function orign_code_show(record){
				record = record.replace(/\t/g, '&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp');
				record = record.replace(/\n/g, '</br>');
				
				$("#orign_code").html("");
				$("#orign_code").append(record);
				var str=document.getElementById("orign_code").innerHTML;
				str=str.replace(/InitializeGraph/,"<font color=\"red\">InitializeGraph</font>");
				str=str.replace(/VertexFilter/,"<font color=\"red\">VertexFilter</font>");
				str=str.replace(/ProcessEdge/,"<font color=\"red\">ProcessEdge</font>");
				str=str.replace(/AccumulateEdge/,"<font color=\"red\">AccumulateEdge</font>");
				str=str.replace(/AccumulateVertex/,"<font color=\"red\">AccumulateVertex</font>");
				str=str.replace(/print/,"<font color=\"red\">print</font>");
				str=str.replace(/UserAlgorithm/g,"<font color=\"red\">UserAlgorithm</font>");
				str=str.replace(/ExecuteAlgorithm/g,"<b>ExecuteAlgorithm</b>");
				str=str.replace(/PrintPerformance/g,"<b>PrintPerformance</b>");
				document.getElementById("orign_code").innerHTML=str;
			}
			function generate_code_show(record){
				//record = record.replace(/\s/g, '&nbsp&nbsp');
				record = record.replace(/\t/g, '&nbsp&nbsp&nbsp&nbsp');
				record = record.replace(/\n/g, '</br>');
				record = record.replace(/\s/g, '&nbsp');
				
				$("#generate_code").html("");
				$("#generate_code").append(record);
			}

