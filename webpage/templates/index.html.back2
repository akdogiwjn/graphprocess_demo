	<script src="{% static 'jquery/jquery-3.1.1.min.js' %} "></script>
	

	<script>
	
	var acc_count=0;

	$(document).ready(function(){
		
		$("#span_refresh").bind("click", function () {
			//console.log('reset Accelerator');
			document.getElementById("loading_terminal").style.display="";
			$("#acc_run_ans").html("Accelerator is resetting...");
			$.ajax({
				url:"/reset_run/",
				type:"GET",
				data:{"info":'reset Accelerator'},
				success:function (data) {
					$("#acc_run_ans").html("");		
					for(var i = 0; i<= data.length-1; i++){
						$("#acc_run_ans").append(data[i] + '</br>')
					}
					$("#acc_run_ans").append('Accelerator reset complete' + '</br>')
					document.getElementById("loading_terminal").style.display="none";
				}
			});
		});

		$("#code_show").on("click", function () {
				$.get("{% static 'code_show/acc.v' %}?ti=" + new Date().getTime(), function(record) {
					//$("#acc_code").val(record);
					record = record.replace(/\t/g, '&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp');
					record = record.replace(/\n/g, '</br>');
					
					$("#acc_code").html("");
					$("#acc_code").append(record);
					var str=document.getElementById("acc_code").innerHTML;
					str=str.replace(/InitializeGraph/,"<font color=\"red\">InitializeGraph</font>");
					str=str.replace(/module/g,"<b>module</b>");
					str=str.replace(/begin/g,"<b>begin</b>");
					str=str.replace(/end/g,"<b>end</b>");
					str=str.replace(/for/g,"<b>for</b>");
					str=str.replace(/if/g,"<b>if</b>");
					str=str.replace(/else/g,"<b>else</b>");
					str=str.replace(/reg/g,"<b>reg</b>");
					str=str.replace(/const/g,"<b>const</b>");
					document.getElementById("acc_code").innerHTML=str;
				});
		});

		$("#accelerator_reset").on("click", function () {
			var chart = echarts.init(document.getElementById("bfs_bardata"));
			chart.dispose();
			chart = echarts.init(document.getElementById("pr_bardata"));
			chart.dispose();
			chart = echarts.init(document.getElementById("bfs_power_bardata"));
			chart.dispose();
			chart = echarts.init(document.getElementById("pr_time_bardata"));
			chart.dispose();
			$("#acc_run_ans").html("");
			//$("#acc_code").val('');
			$("#acc_code").html("");

			myColumns['x_data'] = [];
			myColumns['foregraph_data'] = [];
			myColumns['graphops_data'] = [];
			myColumns['midterm_data'] = [];
			myColumns['acc_data'] = [];
			myColumns['final_data'] = [];

			mypowerColumns['x_data'] = [];
			mypowerColumns['foregraph_data'] = [];
			mypowerColumns['midterm_data'] = [];
			mypowerColumns['acc_data'] = [];
			mypowerColumns['final_data'] = [];

			prColumns['x_data'] = [];
			prColumns['foregraph_data'] = [];
			prColumns['graphops_data'] = [];
			prColumns['midterm_data'] = [];
			prColumns['acc_data'] = [];
			prColumns['final_data'] = [];

			prtimeColumns['x_data'] = [];
			prtimeColumns['foregraph_data'] = [];
			prtimeColumns['midterm_data'] = [];
			prtimeColumns['acc_data'] = [];
			prtimeColumns['final_data'] = [];
		});
		
		$("#accelerator_run").on("click", function () {

			/*if($("#algorithm").val() == "bfs"){
				for(var i=0;i<myColumns.x_data.length;i++){
					if($("#dataset").val()==myColumns.x_data[i]){
						console.log('same algorithm and dataset');
						return;
					}
				}
			}
			else if($("#algorithm").val() == "pagerank"){
				for(var i=0;i<prColumns.x_data.length;i++){
					if($("#dataset").val()==prColumns.x_data[i]){
						console.log('same algorithm and dataset');
						return;
					}
				}
			}*/
			
			if($("#algorithm").val() == 'none' || $("#dataset").val()=='none'){
				return;
			}
			

			$("#acc_run_ans").html("");

			//$("#loading_div").show();
			if($("#algorithm").val() == "bfs"){
				document.getElementById("loading_terminal").style.display="";
				document.getElementById("loading_bfs_bardata").style.display="";
				document.getElementById("loading_bfs_power_bardata").style.display="";
			}
			else if($("#algorithm").val() == "pagerank"){
				document.getElementById("loading_terminal").style.display="";
				document.getElementById("loading_pr_bardata").style.display="";
				document.getElementById("loading_pr_time_bardata").style.display="";
			}
			
			acc_count+=1;
			acc_count=acc_count%2;
			$.ajax({
				url:"/acc_run/",
				type:"GET",
				data:{"dataset":$("#dataset").val(),"algorithm":$("#algorithm").val(),"acc_count":acc_count},
				success:function (data) {
					$("#acc_run_ans").html("");

					for(var i = 0; i<= data.length-1; i++){
						if(JSON.stringify(data[i]).substr(2,10)=='XILINX_XRT'
							||JSON.stringify(data[i]).substr(2,4)=='PATH'
							||JSON.stringify(data[i]).substr(2,15)=='LD_LIBRARY_PATH'
							||JSON.stringify(data[i]).substr(2,10)=='PYTHONPATH'
							||JSON.stringify(data[i]).substr(2,6)=='/home/'){
							continue;
						}
						$("#acc_run_ans").append(data[i] + '</br>');
						console.log(JSON.stringify(data[i]));
					}

					if($("#algorithm").val() == "bfs"){
						myColumns.x_data.push($("#dataset").val());
						myColumns.foregraph_data.push(0.33);
						myColumns.graphops_data.push(0.066);
						myColumns.midterm_data.push(0.5);
						myColumns.acc_data.push(JSON.stringify(data[data.length-2]).substr(44,9));
						myColumns.final_data.push(1.0);
						draw_bfs();

						mypowerColumns.x_data.push($("#dataset").val());
						mypowerColumns.foregraph_data.push(0.002);
						mypowerColumns.midterm_data.push(0.010);
						mypowerColumns.acc_data.push(parseFloat(parseFloat(JSON.stringify(data[data.length-2]).substr(44,9))/28.76).toFixed(3));
						mypowerColumns.final_data.push(0.030);
						draw_bfs_power();

						document.getElementById("loading_terminal").style.display="none";
						document.getElementById("loading_bfs_bardata").style.display="none";
						document.getElementById("loading_bfs_power_bardata").style.display="none";
					}
					else if($("#algorithm").val() == "pagerank"){
						prColumns.x_data.push($("#dataset").val());
						prColumns.foregraph_data.push(1.0);
						prColumns.graphops_data.push(0.2);
						prColumns.midterm_data.push(1.22);
						prColumns.acc_data.push(JSON.stringify(data[data.length-2]).substr(44,9));
						prColumns.final_data.push(1.92);
						draw_pr();

						prtimeColumns.x_data.push($("#dataset").val());
						prtimeColumns.foregraph_data.push(70);
						prtimeColumns.midterm_data.push(55);
						var str=JSON.stringify(data[data.length-3]);
						console.log(str);
						var str2=str.substr(38,str.length-5-31);
						console.log(str2);
						str3=parseFloat(parseFloat(str2)/1000000).toFixed(2);
						prtimeColumns.acc_data.push(str3);
						prtimeColumns.final_data.push(35);
						draw_pr_time();

						document.getElementById("loading_terminal").style.display="none";
						document.getElementById("loading_pr_bardata").style.display="none";
						document.getElementById("loading_pr_time_bardata").style.display="none";
					}
					
					//$("#loading_div").hide();

				}
			});
		});
	});
	</script>


	<script>

		myColumns = {};
		myColumns['x_data'] = [];
		myColumns['foregraph_data'] = [];
		myColumns['graphops_data'] = [];
		myColumns['midterm_data'] = [];
		myColumns['acc_data'] = [];
		myColumns['final_data'] = [];

		function draw_bfs(){

			var myChart = echarts.init(document.getElementById('bfs_bardata'));
			
			var colors = ['#000000', '#D9B300', '#FF0000', '#4A4AFF', '#ADADAD'];

			option = {
				color: colors,
			tooltip: {
				trigger: 'axis',
				axisPointer: {
					type: 'cross'
				}
			},
			grid: {
				right: '10%'
			},
			toolbox: {
				feature: {
					dataView: {show: true, readOnly: true},
					restore: {show: false},
					saveAsImage: {show: true}
				}
			},
			legend: {
				data:['Foregraph','GraphOps','中期指标','加速器', '最终指标']
			},
			xAxis: [
				{
					type: 'category',
					axisTick: {
						aignWithLabel: true
					},
					data: myColumns.x_data
				}
			],
			yAxis: [
				{
					type: 'value',
					name: '性能(GTEPS)',
					min: 0.0,
					max: 2.0,
					position: 'left',
					axisLine: {
						lineStyle: {
							color: colors[0]
						}
					},
					axisLabel: {
						formatter: '{value} '
					}
				},
				{
					type: 'value',
					name: '',
					min: 0.0,
					max: 2.0,
					position: 'right',
					axisLine: {
						lineStyle: {
							color: colors[2]
						}
					},
					axisLabel: {
						formatter: '{value} '
					}
				}
			],
			series: [
				{
					name:'Foregraph',
					type:'bar',
					data: myColumns.foregraph_data
				},
				{
					name:'GraphOps',
					type:'bar',
					data: myColumns.graphops_data

				},
				{
					name:'中期指标',
					type:'bar',
					yAxisIndex: 1,
					data: myColumns.midterm_data
				},
				{
					name:'加速器',
					type:'bar',
					yAxisIndex: 1,
					data: myColumns.acc_data
				},
				{
					name:'最终指标',
					type:'bar',
					yAxisIndex: 1,
					data: myColumns.final_data
				}
				]
			};
				myChart.setOption(option);

	}

	</script>

